class Result {

    public static int marsExploration(String s) {
        int count=0;
        for(int i=0;i<s.length();i++){
            if((i-1)%3==0 && i>1 || i==1){
                if(s.charAt(i)!='O'){count++;}
            }
            else if(s.charAt(i)!='S'){
                count++;
            }
        }
        return count;
    }

}

//all SOS language is multiple of SOS.
